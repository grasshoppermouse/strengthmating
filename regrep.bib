  volume = {9},
  pages = {e62883},
  publisher = {{eLife Sciences Publications, Ltd}},
  issn = {2050-084X},
  doi = {10.7554/eLife.62883},
  urldate = {2021-01-23},
  abstract = {In comparative cross-species perspective, humans experience unique physical impairments with potentially large consequences. Quantifying the burden of impairment in subsistence populations is critical for understanding selection pressures underlying strategies that minimize risk of production deficits. We examine among forager-horticulturalists whether compromised bone strength (indicated by fracture and lower bone mineral density, BMD) is associated with subsistence task cessation. We also estimate the magnitude of productivity losses associated with compromised bone strength. Fracture is associated with cessation of hunting, tree chopping, and walking long distances, but not tool manufacture. Age-specific productivity losses from hunting cessation associated with fracture and lower BMD are substantial: \textasciitilde 397 lost kcals/day, with expected future losses of up to 1.9 million kcals (22\% of expected production). Productivity loss is thus substantial for high strength and endurance tasks. Determining the extent to which impairment obstructs productivity in contemporary subsistence populations improves our ability to infer past consequences of impairment.},
  keywords = {bone mineral density,disability,functional disability,life history theory,productivity,skeletal gracility,to read,tsimane,vertebral fracture,via ed},
  file = {/Users/carolinesmith/Zotero/storage/ST8UUPYB/Stieglitz et al. - 2020 - Productivity loss associated with functional disab.pdf}
}

