@article{pack2022,
  title = {Transforming {{Possible Risk Into Certain Harm}}: {{A Critical Interpretive Synthesis}} of the {{Literature}} on {{Perinatal Cannabis Use}}},
  shorttitle = {Transforming {{Possible Risk Into Certain Harm}}},
  author = {Pack, Rachael and Hilton, Grace and {Garcia-Bournissen}, Facundo and Taylor, Taryn},
  year = {2022},
