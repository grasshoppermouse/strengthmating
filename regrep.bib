@incollection{swynghedauw2016,
  title = {Evolutionary {{Paradigms}} in {{Cardiology}}: {{The Case}} of {{Chronic Heart Failure}}},
  shorttitle = {Evolutionary {{Paradigms}} in {{Cardiology}}},
  booktitle = {Evolutionary {{Thinking}} in {{Medicine}}: {{From Research}} to {{Policy}} and {{Practice}}},
  author = {Swynghedauw, Bernard},
  editor = {Alvergne, Alexandra and Jenkinson, Crispin and Faurie, Charlotte},
  year = {2016},
  series = {Advances in the {{Evolutionary Analysis}} of {{Human Behaviour}}},
  pages = {137--153},
  publisher = {{Springer International Publishing}},
  address = {{Cham}},
  doi = {10.1007/978-3-319-29716-3_10},
  urldate = {2020-04-15},
  abstract = {Chronic heart failure (HF) occurs after a long period of cardiac remodelling, during which the heart manages to compensate due to changed working conditions induced by cardiovascular diseases (CVDs). Cardiac hypertrophy and the reduction of the contractile capacity of the heart (V max) are the main components of this adaptive process. Cardiac hypertrophy is adaptive both by increasing the number of contractile units and by reducing the wall stress. The reduction of V max is adaptive by normalizing cardiac muscle economy. From an evolutionary perspective, the major goal of chronic HF treatment is thus to improve muscle economy, either by suppressing the cause of cardiac load (e.g. by treating valve disease or hypertension) or by reducing the loading conditions (e.g. with anti-aldosterone drugs, converting enzyme inhibitors or diuretics). Furthermore, an evolutionary perspective suggests that the prescription of positive inotropic agents to increase cardiac contractility is not indicated in chronic HF, as opposed to the case for acute HF. Thinking evolutionarily also provides new avenues for the establishment of prognostics and diagnostic biomarkers of HF. For instance, a comparative analysis reveals that the helical cardiac structure observed in every tetrapod is an adaptive cardiac feature. Thus, given that the early stages of cardiac overload are characterized by the impairment of systolic torsion, the quantification of cardiac torsion may provide a better marker for HF than the ejection fraction that is currently utilized by every clinician.},
  isbn = {978-3-319-29716-3},
  langid = {english},
  keywords = {Aged heart,Cardiac remodelling,Cardiac torsion,Evolutionary cardiology,Heart failure,Mechanotransduction},
  file = {/Users/carolinesmith/Zotero/storage/GL9IXEIX/Swynghedauw_2016_Evolutionary Paradigms in Cardiology.pdf}
}

@article{syddall2003,
  ids = {syddall2003a},
  title = {Is Grip Strength a Useful Single Marker of Frailty?},
