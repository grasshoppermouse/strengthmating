}

@incollection{sellers2016,
  title = {Is {{Calculus Relevant}} to {{Survival}}? {{Managing}} the {{Evolutionary Novelty}} of {{Modern Education}}},
  shorttitle = {Is {{Calculus Relevant}} to {{Survival}}?},
  booktitle = {Evolutionary {{Thinking}} in {{Medicine}}: {{From Research}} to {{Policy}} and {{Practice}}},
  author = {Sellers, P. Douglas and Machluf, Karin and Bjorklund, David F.},
  editor = {Alvergne, Alexandra and Jenkinson, Crispin and Faurie, Charlotte},
  year = {2016},
  series = {Advances in the {{Evolutionary Analysis}} of {{Human Behaviour}}},
  pages = {89--102},
  publisher = {{Springer International Publishing}},
  address = {{Cham}},
  doi = {10.1007/978-3-319-29716-3_7},
  urldate = {2020-04-15},
  abstract = {Humans have always educated their children, but until recently education was done ``hands-on'' by a parent, familiar adult, or more competent peer. It has only been within the last few centuries that a significant number of the world's children must learn some culturally important context-independent skills, such as reading, writing, and mathematics. These skills are usually taught to children by unfamiliar adults in unfamiliar settings. The result is that much of formal education is an evolutionary novelty. Sitting in classrooms, reading books, or memorizing multiplication tables were certainly not requisite skills for our evolutionary forechildren to master. Yet such evolutionarily novel tasks are critical for functioning in modern societies, and children are quite capable of achieving such feats given appropriate attention and training. However, this leaves many contemporary societies with discrepancies between conditions of modern life and the evolutionary skill set endowed to each child by human cognitive evolution. It is the job of education to bridge this gap, and just as an evolutionary perspective can inform the study of child development at large, perhaps it can also inform educational practices. This chapter presents a brief summary of current common education practice and theory, followed by a description of theory and research underlying an evolutionary approach to education in an effort to prompt discussion regarding the merits and applicability of evolutionary reasoning to education.},
  isbn = {978-3-319-29716-3},
  langid = {english},
  keywords = {Biologically primary,Biologically secondary,Child-centered,Development,Education practices,Evolutionary approach,Evolutionary educational psychology},
  file = {/Users/carolinesmith/Zotero/storage/VSIVVRN7/Sellers et al_2016_Is Calculus Relevant to Survival.pdf}
}

