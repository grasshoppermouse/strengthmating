  author = {{van Eeden}, Wessel A. and {van Hemert}, Albert M. and Carlier, Ingrid V. E. and Penninx, Brenda W. J. H. and Lamers, Femke and Fried, Eiko I. and Schoevers, Robert and Giltay, Erik J.},
