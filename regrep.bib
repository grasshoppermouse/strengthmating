@article{saxbe2017,
  title = {High Paternal Testosterone May Protect against Postpartum Depressive Symptoms in Fathers, but Confer Risk to Mothers and Children},
  author = {Saxbe, Darby E. and Schetter, Christine Dunkel and Simon, Clarissa D. and Adam, Emma K. and Shalowitz, Madeleine U.},
