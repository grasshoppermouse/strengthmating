  shorttitle = {Sex {{Differences}} in {{Depressive}} and {{Socioemotional Responses}} to an {{Inflammatory Challenge}}},
  author = {Moieni, Mona and Irwin, Michael R and Jevtic, Ivana and Olmstead, Richard and Breen, Elizabeth C and Eisenberger, Naomi I},
