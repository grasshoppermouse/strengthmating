@article{nolin2012,
  title = {Food-Sharing Networks in {{Lamalera}}, {{Indonesia}}: Status, Sharing, and Signaling},
  shorttitle = {Food-Sharing Networks in {{Lamalera}}, {{Indonesia}}},
  author = {Nolin, David A.},
  year = {2012},
