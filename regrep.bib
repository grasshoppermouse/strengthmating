  shorttitle = {Best {{Practices}} for {{Developing}} and {{Validating Scales}} for {{Health}}, {{Social}}, and {{Behavioral Research}}},
  author = {Boateng, Godfred O. and Neilands, Torsten B. and Frongillo, Edward A. and {Melgar-Qui{\~n}onez}, Hugo R. and Young, Sera L.},
