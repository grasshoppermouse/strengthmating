  title = {{{POLYGYNY}}, {{REPRODUCTIVE SUCCESS AND CHILD HEALTH IN RURAL ETHIOPIA}}: {{WHY MARRY A MARRIED MAN}}?},
  shorttitle = {{{POLYGYNY}}, {{REPRODUCTIVE SUCCESS AND CHILD HEALTH IN RURAL ETHIOPIA}}},
