@article{dunsworth2020,
  ids = {dunsworthExpandingEvolutionaryExplanations2020},
  title = {Expanding the Evolutionary Explanations for Sex Differences in the Human Skeleton},
  author = {Dunsworth, Holly M.},
  year = {2020},
