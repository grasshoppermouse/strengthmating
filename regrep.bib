@article{crume2018,
  title = {Cannabis {{Use During}} the {{Perinatal Period}} in a {{State With Legalized Recreational}} and {{Medical Marijuana}}: {{The Association Between Maternal Characteristics}}, {{Breastfeeding Patterns}}, and {{Neonatal Outcomes}}},
  shorttitle = {Cannabis {{Use During}} the {{Perinatal Period}} in a {{State With Legalized Recreational}} and {{Medical Marijuana}}},
  author = {Crume, Tessa L. and Juhl, Ashley L. and {Brooks-Russell}, Ashley and Hall, Katelyn E. and Wymore, Erica and Borgelt, Laura M.},
