@article{lopezsteinmetz2019,
  ids = {lopezsteinmetz2019a},
  title = {Perfil de M\'etodos En Intentos de Suicidio: Tendencias e Implicancias Para La Prevenci\'on. {{Jujuy}}, Noroeste de {{Argentina}}},
