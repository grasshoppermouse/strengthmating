@article{plavcan1995,
  title = {Competition, Coalitions and Canine Size in Primates},
  author = {Plavcan, J. Michael and {van Schaik}, Carel P. and Kappeler, Peter M.},
  year = {1995},
  month = mar,
  journal = {Journal of Human Evolution},
  volume = {28},
  number = {3},
