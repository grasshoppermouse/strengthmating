@article{datta2018,
  title = {Decolonizing Both Researcher and Research and Its Effectiveness in {{Indigenous}} Research},
  author = {Datta, Ranjan},
  year = {2018},
  month = apr,
  journal = {Research Ethics},
  volume = {14},
