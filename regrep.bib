  keywords = {Dominance,Groups,Hierarchy,Inequality,Status},
  file = {/Users/carolinesmith/Zotero/storage/XNYGFMQ2/Anderson_Willer_2014_Do Status Hierarchies Benefit Groups.pdf}
}

@incollection{anderson2014a,
