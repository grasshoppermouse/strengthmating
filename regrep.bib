@article{woody,
  title = {Displays of Negative Facial Affect during Parent\textendash Adolescent Conflict and the Bidirectional Transmission of Social Anxiety},
  author = {Woody, Mary L. and Kaurin, Aleksandra and McKone, Kirsten M.P. and Ladouceur, Cecile D. and Silk, Jennifer S.},
  journal = {Journal of Child Psychology and Psychiatry},
  volume = {n/a},
  number = {n/a},
