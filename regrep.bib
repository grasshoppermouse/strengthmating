@incollection{laue2019,
  title = {Landscape {{Revolutions}} for {{Cultural Evolution}}: {{Integrating Advanced Fitness Landscapes}} into the {{Study}} of {{Cultural Change}}},
  shorttitle = {Landscape {{Revolutions}} for {{Cultural Evolution}}},
  booktitle = {Handbook of {{Evolutionary Research}} in {{Archaeology}}},
  author = {Laue, Cheyenne L. and Wright, Alden H.},
