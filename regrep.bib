  issn = {2045-2322},
  doi = {10.1038/s41598-018-25694-0},
  urldate = {2021-06-14},
  abstract = {The observations that testosterone might be immunosuppressive, form the basis for the immunocompetence handicap hypothesis (ICHH). According to ICHH only high-quality individuals can maintain high levels of testosterone and afford the physiological cost of hormone-derived immunosuppression. The animal and human studies that attempted to support the ICHH by precisely defined impairment of immunity associated with high testosterone levels are inconclusive. Furthermore, human studies have used only selected immune functions and varying testosterone fractions. This is the first study examining the relationship between multiple innate and adaptive immunity and serum levels of free testosterone, total testosterone, DHT and DHEA in ninety-seven healthy men. Free testosterone and marginally DHT levels were positively correlated with the strength of the influenza post-vaccination response. Total testosterone and DHEA showed no immunomodulatory properties. Our findings did not support ICHH assumptions about immunosuppressive function of androgens. In the affluent society studied here, men with higher levels of free testosterone could afford to invest more in adaptive immunity. Since the hormone-immune relationship is complex and may depend on multiple factors, including access to food resources, androgens should be treated as immunomodulators rather than implicit immunosuppressants.},
  copyright = {2018 The Author(s)},
  langid = {english},
  keywords = {Evolutionary theory,Sexual selection},
  file = {/Users/carolinesmith/Zotero/storage/9ZW3ZQMB/Nowak et al. - 2018 - No evidence for the immunocompetence handicap hypo.pdf}
}

