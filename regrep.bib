@article{nuzzo2023,
  title = {Narrative {{Review}} of {{Sex Differences}} in {{Muscle Strength}}, {{Endurance}}, {{Activation}}, {{Size}}, {{Fiber Type}}, and {{Strength Training Participation Rates}}, {{Preferences}}, {{Motivations}}, {{Injuries}}, and {{Neuromuscular Adaptations}}},
  author = {Nuzzo, James L.},
