  journal = {European Journal of Personality},
  volume = {31},
  number = {6},
  pages = {658--668},
  publisher = {{SAGE Publications Ltd}},
  issn = {0890-2070},
  doi = {10.1002/per.2117},
  urldate = {2021-05-26},
  abstract = {Researchers applying clustering methods have found that the five most commonly studied personality traits (the `Big Five') appear to form three prototypes, known as resilient, undercontrolled, and overcontrolled (RUO) personality types. The analysis has been replicated cross\textendash nationally, and the results have been reasonably robust. However, these findings do not necessarily imply discontinuities or non\textendash linearities in the Big Five data. We study whether the RUO types can arise from typical Big Five intercorrelations alone. We used data from a previous meta\textendash analysis of inter\textendash trait correlations (N = 144 117 participants) and simulated normally distributed observations with this correlation structure. Applying three different clustering algorithms (k\textendash means, hierarchical agglomerative, and model based) with three\textendash cluster solutions to the simulated data, we examined whether the known correlations alone can give rise to the RUO typology. The simulated results were compared with previous empirical findings. A simple multivariate normal distribution with the Big Five correlation structure was sufficient to generate the RUO typology in three\textendash cluster solutions for all the three clustering methods. Contrary to the RUO typology `carving personality description at its joints', linear correlations typical for correlations among Big Five traits can create RUO types even in the absence of any points of discontinuity. Copyright \textcopyright{} 2017 European Association of Personality Psychology},
  langid = {english},
  keywords = {clustering methods,five-factor inventory,personality types,statistical methods}
}

