  langid = {english},
  keywords = {Genetics research,Genome-wide association studies,Quantitative trait loci,via ed},
  file = {/Users/carolinesmith/Zotero/storage/CS9VZLM4/Willems et al_2017_Large-scale GWAS identifies multiple loci for hand grip strength providing.pdf}
}

