@article{stockman2014,
  title = {Intimate {{Partner Violence}} and {{Its Health Impact}} on {{Ethnic Minority Women}}},
  author = {Stockman, Jamila K. and Hayashi, Hitomi and Campbell, Jacquelyn C.},
